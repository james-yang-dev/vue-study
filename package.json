{
  "name": "web-parser",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --host 0.0.0.0 --disabledHostCheck true",
    "api": "nodemon src/data/server.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "jszip": "^3.4.0",
    "lodash": "^4.17.15",
    "shortid": "^2.2.15",
    "vue": "^2.6.11",
    "vue-lodash": "^2.1.2",
    "vue-radial-progress": "^0.3.1",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "axios": "^0.18.0",
    "json-server": "^0.16.1",
    "lowdb": "^1.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-axios": "~0.0.4",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "ignore",
          "named": "ignore",
          "asyncArrow": "ignore"
        }
      ],
      "quotes": [
        "error",
        "single",
        {
          "allowTemplateLiterals": true
        }
      ],
      "no-unused-vars": "off",
      "prefer-const": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
